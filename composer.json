        "kevintweber",
        "groundskeeper",
        "html",
        "sanitize",
        "filter"
    ],
    "homepage": "https://github.com/kevintweber/Groundskeeper",
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Weber",
            "email": "kevintweber@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^5.5.9|^7.0",
        "kevintweber/html-tokenizer" : "^0.3",
        "psr/log" : "^1.0",
        "symfony/options-resolver" : "^2.8|^3.0"
    },
    "autoload": {
        "psr-4": {
